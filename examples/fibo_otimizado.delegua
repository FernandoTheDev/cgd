importar "io"

função fibonacci(n: inteiro): inteiro {
    se n <= 1 retorna n

    var a, b, temp = 0, 1, 0
    
    para var i = 0; i < n - 1; i++ {
        temp = a + b
        a = b
        b = temp
    }
    
    retorna b
}

var resultado = fibonacci(100)
escrevaln('resultado: ', resultado)

se (resultado != 3736710778780434371) {
    escrevaln('o resultado está errado!')
}
